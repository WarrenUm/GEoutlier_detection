   "source": [
    "#function to take a datetime and convert it to unix time.\n",
    "import time\n",
    "import datetime"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "bb8bb2ce-1e30-47ba-8ca0-ff72d915645e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def unix_conv(time_string):\n",
    "    unix = time.mktime(datetime.datetime.strptime(time_string, \"%d/%m/%Y\").timetuple())\n",
    "    \n",
    "    return unix"
   ]
  },
